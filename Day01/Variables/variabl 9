#include <stdio.h>
#include <math.h>
int main() {
   float x, y, z ,dis ;
   printf("entre x = ");
   scanf("%f",&x);
   printf("entre y = ");
   scanf("%f",&y);
   printf("entre z = ");
   scanf("%f",&z);

   dis = sqrt (pow(2x-1x,2) + pow(2y-1y,2) + pow(2z-1z,2));

  printf("diistance=%f",dis);
}
